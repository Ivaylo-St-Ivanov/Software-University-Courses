<main>
    <h1>Browser</h1>
    <form action="" method="">
        <input type="text" class="search" name="search" placeholder="Search...">
        <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
        <span>-</span>
        <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
        <input type="submit" value="search">
    </form>
    {{#each data}}
    <div class="cube">
        <img class="cube" src="{{imageUrl}}">
        <p class="name">{{name}}</p>
        <p><span>Difficulty level:</span> {{level}}</p>
        <a class="btn" href="/details/{{id}}">Details</a>
    </div>
    {{else}}
    <h1>No cubes were found</h1>
    {{/each}}
</main>